// Tetun Dili phonemes (based on Indonesian and Portuguese)

phoneme a
  vwl starttype #a endtype #a
  length 180
  FMT(vowel/a_2)
endphoneme

phoneme E // low E
  vwl starttype #e endtype #e
  length 180
  FMT(vowel/e_mid)
endphoneme

phoneme e // high /e/
  vwl starttype #e endtype #e
  length 180
  FMT(vowel/e)
endphoneme

phoneme i
  vwl starttype #i endtype #i
  length 180
  FMT(vowel/i)
endphoneme

phoneme O // low 
  vwl starttype #o endtype #o
  length 180
  FMT(vowel/0_2)
endphoneme

phoneme o // high
  vwl starttype #o endtype #o
  length 180
  FMT(vowel/o)
endphoneme

phoneme u
  vwl starttype #u endtype #u
  length 180
  FMT(vowel/u_6)
endphoneme