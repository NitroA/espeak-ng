// This file is UTF8 encoded
// Spelling to phoneme rules for Crimean Tatar

.replace 
        а	a
        б	b
        к	c
        д	d
        е	e
        ф	f
        г	g
        гъ ğ
        х	h
        и	i
        ж	j
        л	l
        м	m	
        н	n
        нъ ñ
        о	o
        п	p
        р	r
        с	s
        т	t
        у	u
        в	v
        ш	x
        з	z
        ё	yo
        й	y
        ц	ts
        ч	ç
        дж c
        щ	şç
        ы	ı
        э	e
        ю	ü
        я	â
        
.group a
        a         a
        
.group â
        â         ja
        
.group b
        b         b
        bb        b:
        
.group c
        c         DZ
        cc        DZ:
        
.group ç
        ç         TS
        çç        TS:
        
.group d
        d          d
        dd         d:
        
.group e
        e          e
        
.group f
        f          f
        
.group g
        g          g
        
.group ğ
        ğ          G
        
.group h
        h          x
        
 .group ı
         ı         I
         
 .group i
         i         i
 
 .group j
         j         j
         
 .group k
         k         k
         kk        k:
         
 .group l
        l          l
        l (L02     L
   L02) l (K       L
         
.group m
        m          m
        
.group n
        n          n
        
.group ñ
        ñ          N
        
.group o
        o          o
        
.group ö
        ö          O
        
.group p
        p          p
        pp         p:
        
.group q
        q          q
        qq         q:
         
.group r
        r          r
        
.group s
        s          s
        
.group ş
        ş          S
        
.group t
        t          t
        tt         t:
        
.group u
        u          u
        
.group ü
        ü          U
        
.group v
        v          v
        
 .group y
         y         y
         
 .group z
         z         z
     
 .group
        $        doL'ar        
         
