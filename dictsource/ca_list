q	k'u
r	'eR@
s	'es@
t	t'e
v	'v#'e||b'aS@
w	'v#'e||d'obbl@
x	iks
y	'i||g**EQ@
z	z'Et@


// pronunciation exceptions

moc m'ok    $noun
dona    d'on@   $verb
coure k'ow**@    $noun

