// function words

// articles
el	@l   $nounf
la	l@   $nounf
els	$u  $nounf
les	$u  $nounf
un	$u   $nounf
una	'un@    $nounf
uns	$u  $nounf
unes	$u $nounf

// pronoms febles
hi  $u  $verbf
ho  $u  $verbf
em  $u  $verbf
me  $u  $verbf
et  $u  $verbf
te  $u  $verbf
lo  $u  $verbf
en  $u  $verbf
ne  $u  $verbf
li  $u  $verbf
ens $u  $verbf
nos $u  $verbf
us  $u  $verbf
vos $u  $verbf
los $u  $verbf
es  $u  $verbf
se  $u  $verbf



// prepositions
a	$u $brk
al	$u $brk $nounf
als	$u $brk $nounf
de	$u
del	$u  $nounf
dels	$u $nounf
en      $u $brk
per	$u $brk
pel	$u $brk $nounf
pels	$u $brk    $nounf
ans $u

amb	$brk
entre	$brk


// conjunctions
i	$u $pause
o       $alt2 $pause
que $u


// auxiliary verbs
sóc	s'ok 
