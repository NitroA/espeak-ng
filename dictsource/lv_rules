.group i
           i (A            i_!
           ie              ie
           i               i
      ikv) ie (n           'ie       // stress on 2nd syllable
           iu              iu

.group ī
           ī               i:

.group j
           j               j
        _) jebk            jebk'     // stress on 2nd syllable

.group k
           k               k

.group ķ
           ķ               c

.group l
           l               l
           ll              lll       // espeak says doubled ll shorter than doubled ļļ

.group ļ
           ļ               l^

.group m
           m               m

.group n
        _) ne (vien        ne'       // stress on 2nd syllable
           n (g            N
           n (k            N
           n               n
           nn              nn:       // espeak says doubled nn shorter that doubled ņņ

.group ņ
           ņ               n^

.group o
   // default policy rules
        o) o               o:        // in ..oo.. second o is spelled as ō
           o ($w_alt       o         // alt words ($alt list in lv_list) are spelled as o
           o (_$w_alt      o:        // o at the end of alt words is spelled as ō

      L42) o               o:        // o prefixes
           o (L43          o         // o postfixes

L41L10L10L10L10L10) o      o         // o indicators before
   L41L10L10L10L10) o      o
